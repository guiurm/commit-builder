{
  "name": "@guiurm/taskgit",
  "version": "1.0.0-rc.2",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run build:ts && node build/index.js",
    "dev2": "ts-node -r tsconfig-paths/register src/index.ts commit -t fix -m \"test test\"",
    "dev3": "ts-node -r tsconfig-paths/register src/index.ts config-user",
    "format": "prettier --ignore-path .prettierignore --write src/",
    "clearBuild": "rimraf dist build",
    "build:ts": "npm run clearBuild && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build": "npm run format && npm run build:ts && rollup -c "
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "taskgit": "./dist/index.cjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@guiurm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.10.2",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@guiurm/askly": "^1.0.1",
    "@guiurm/termify": "^1.1.1"
  }
}
